{
	"name": "klipper",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "packages/klipper/src",
	"targets": {
		"build": {
			"executor": "@vscode-devkit/nx:build",
			"options": {
				"additionalTargets": [
					"klipper-cfg:build",
					"klipper-gcode:build",
					"klipper-script:build"
				],
				"assets": [
					"LICENSE",
					"package.json",
					"README.md"
				],
				"entryPoint": "src/index.ts",
				"outputPath": "dist/packages/vscode-klipper",
				"outputFile": "main.js",
				"package": true,
				"install": true
			}
		}
	},
	"implicitDependencies": [
		"klipper-cfg",
		"klipper-gcode",
		"klipper-script"
	],
	"tags": []
}
